#include <Wire.h>
void setup() {
  Wire.begin();
  Serial.begin(9600);
}
void loop() {
  Wire.beginTransmission(0x48);  // Mock address
  Wire.write(0x00);
  Wire.endTransmission();
  Wire.requestFrom(0x48, 1);
  int data = Wire.read();
  Serial.print("Sensor Value: "); Serial.println(data);
  delay(1000);
}
